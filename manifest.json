{
  "manifest_version": 2,
  "name": "Melvor Auto-Prayer",
  "version": "1.0",

  "description": "Allows you to map what prayers you want active during which enemy spell casts",

  "icons": {
      "48": "icons/border-48.png"
  },

  "content_scripts": [
      {
          "matches": ["*://*.melvoridle.com/*"],
          "js": ["auto-prayer.js"]
      }
  ],

  "applications": {
      "gecko": {
          "id": "borderify@example.com"
      }
  }
}